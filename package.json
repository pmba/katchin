{
  "name": "pa-farmer",
  "version": "1.1",
  "description": "",
  "main": "run.js",
  "scripts": {
    "watch": "watchify src/scripts/main.js -t babelify -o dist/bundle.min.js",
    "browserify": "browserify src/scripts/main.js -t babelify -o dist/bundle.js",
    "uglify": "uglifyjs dist/bundle.js -c --comments -o dist/bundle.min.js",
    "content": "cp ./src/content.js ./dist/content.js",
    "build": "npm run browserify && npm run uglify",
    "postbuild": "rm -f ./dist/bundle.js"
  },
  "author": "Phyllipe Bezerra",
  "license": "ISC",
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-preset-es2015": "^6.24.1",
    "babelify": "^8.0.0",
    "eslint": "^6.8.0",
    "watchify": "^3.11.1",
    "webpack-cli": "^3.3.11"
  },
  "dependencies": {
    "@types/chrome": "0.0.104",
    "path": "^0.12.7",
    "uglify-es": "^3.3.9",
    "uglify-js-es6": "^2.8.9"
  }
}
